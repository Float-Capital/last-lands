#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/rescript@10.1.4/node_modules/rescript/node_modules:/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/rescript@10.1.4/node_modules:/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/rescript@10.1.4/node_modules/rescript/node_modules:/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/rescript@10.1.4/node_modules:/Users/denham/Documents/projects/envio/code/eth-istanbul-countries/envio-indexer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bsc" "$@"
else
  exec node  "$basedir/../../bsc" "$@"
fi
